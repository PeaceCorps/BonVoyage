// Static navbar
.row
  .col-xs-12.col-md-offset-1.col-md-10
    nav.navbar.navbar-default
      .navbar-header
        // This button will open the tabs in mobile view
        button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        // peace corps logo
        a.navbar-brand(href='http://www.peacecorps.gov/')#pclogo
          img(src='/image/peace-corps.svg')
        a.navbar-brand(href='/dashboard') BonVoyage
      #navbar.navbar-collapse.collapse
        ul.nav.navbar-nav
          if links
            for link in links
              li(class=(link.active ? "active" : ""))
                a(href=link.href)=link.text
        ul.nav.navbar-nav.navbar-right
          li.dropdown
            a.dropdown-toggle(href='#', data-toggle="dropdown", role='button', aria-haspopup='true', aria-expanded='false')
              | Contact
              span.caret
            ul.dropdown-menu
              li
                a(href='mailto:team+support@projectdelta.io') Support
              li
                a(href='mailto:team+feedback@projectdelta.io') Feedback
                
          if !hideLogout
            // This should log user out
            li(class=(profileClass ? profileClass : ''))
              a(href='/profile')= user.name
            li
              a(href='#')#logout Logout
