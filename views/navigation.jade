// Static navbar
.row
  .col-xs-12.col-md-offset-1.col-md-10
    nav.navbar.navbar-default
      .navbar-header
        // This button will open the tabs in mobile view
        button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        // peace corps logo
        a.navbar-brand(href='http://www.peacecorps.gov/')#pclogo
          img(src='/image/peace-corps.svg')
        a.navbar-brand(href='/dashboard') BonVoyage
      #navbar.navbar-collapse.collapse
        ul.nav.navbar-nav
          if links
            for link in links
              li(class=(link.active ? "active" : ""))
                a(href=link.href)=link.text
        if !hideLogout
          ul.nav.navbar-nav.navbar-right
            // This should log user out
            li
              a.nohover= user.name
            li
              a(href='#')#logout Logout

